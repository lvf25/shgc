import QtQuick 2.0
import QtQml 2.0
import QtQuick.Controls         1.2
import QtQuick.Controls.Styles  1.4
import QtQuick.Layouts 1.12



Switch{
    id:shihuanswitch
    opacity: 1
    style:SwitchStyle{
        id:_switchstyle
        groove: Rectangle{
            id:switchstylere
            implicitHeight: shihuanswitch.height
            implicitWidth: shihuanswitch.width
            color: control.checked ?"#E6E9EB":"#03A8F7"
            radius: shihuanswitch.height/2
        }

        handle: Rectangle{
            implicitHeight: shihuanswitch.height
            implicitWidth: shihuanswitch.height
            color: "#FFFFFF"
            radius: shihuanswitch.height/2
        }
    }

}
